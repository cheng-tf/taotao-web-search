<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 消费者应用信息 -->
	<dubbo:application name="taotao-web-search"/>
	<!-- 注册中心 -->
	<dubbo:registry protocol="zookeeper" address="47.106.221.183:2181"/>
	<!-- 引用Dubbo服务 -->
	<dubbo:reference interface="com.taotao.springboot.search.export.SearchResource" id="searchResource" timeout="10000"/>

</beans>